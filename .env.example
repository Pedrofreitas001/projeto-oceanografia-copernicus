# Copernicus Marine Service API Credentials
# Get your credentials at: https://data.marine.copernicus.eu/register
VITE_COPERNICUS_USERNAME=your_copernicus_username
VITE_COPERNICUS_PASSWORD=your_copernicus_password

# Backend URL (leave empty for local development, set for production)
VITE_BACKEND_URL=

# API Mode: 'production' uses real Copernicus data, 'demo' uses mock/fallback
VITE_API_MODE=demo

# ============================================================
# Supabase Configuration (NDBC Pipeline)
# ============================================================
# Get these from: https://supabase.com/dashboard/project/_/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# For the Python ingestion script (server-side only, never expose in frontend)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
